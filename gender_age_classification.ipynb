   "source": [
    "#url of the video to predict Age and gender\n",
    "#url = 'https://www.youtube.com/watch?v=c07IsbSNqfI&feature=youtu.be'\n",
    "#vPafy = pafy.new(url)\n",
    "#play = vPafy.getbest(preftype=\"mp4\") \n",
